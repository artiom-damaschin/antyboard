{
  "extends": [
    "tslint:latest",
    "tslint-react",
    "tslint-react-hooks",
    "tslint-config-airbnb"
  ],
  "rules": {
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "no-console": false,
    "jsx-no-lambda": false,
    "member-ordering": false
  },
  "linterOptions": {
    "exclude": [
      "**/*.js",
      "node_modules/**"
    ]
  }
}
